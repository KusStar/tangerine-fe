{
  "user": {
    "code": 200,
    "user": {
      "userid": "@id()",
      "username": "@cname()",
      "date": "@date()",
      "avatar": "@image('200x200','@color()','#fff','avatar')",
      "description": "@cparagraph()",
      "email": "@email()"
    }
  },
  "tasks": {
    "code": 200,
    "tasks|10-20": [
      {
        "title": "@title()",
        "description": "@paragraph()",
        "date": "@datetime('T')",
        "finished|1": true
      }
    ]
  }
}
